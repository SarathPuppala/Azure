{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "setADFv1"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureSqlDW1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDW1'"
		},
		"LinkFromAzureSqlDBToSqlDW_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkFromAzureSqlDBToSqlDW'"
		},
		"OnPremSQLServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OnPremSQLServer'"
		},
		"OnPremSQLServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'OnPremSQLServer'"
		},
		"SourceBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SourceBlobStorage'"
		},
		"OnPremSQLServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "DSVM2016\\sqladmin"
		},
		"BlobTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/d8713a47-11b8-4f49-906f-d3a19b744cc0/resourceGroups/MYRG/providers/Microsoft.Storage/storageAccounts/setstorageacc"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureSqlDW1_connectionString": {
						"value": "[parameters('AzureSqlDW1_connectionString')]"
					},
					"LinkFromAzureSqlDBToSqlDW_connectionString": {
						"value": "[parameters('LinkFromAzureSqlDBToSqlDW_connectionString')]"
					},
					"SourceBlobStorage_connectionString": {
						"value": "[parameters('SourceBlobStorage_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"OnPremSQLServer_connectionString": {
						"value": "[parameters('OnPremSQLServer_connectionString')]"
					},
					"OnPremSQLServer_password": {
						"value": "[parameters('OnPremSQLServer_password')]"
					},
					"OnPremSQLServer_properties_typeProperties_userName": {
						"value": "[parameters('OnPremSQLServer_properties_typeProperties_userName')]"
					},
					"BlobTrigger_properties_typeProperties_scope": {
						"value": "[parameters('BlobTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}